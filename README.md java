Import Java.util.*;
Public class MBA_chllange
{
 Public static void main(strings args []);
  {
     Scanner sc=new scanner(system.in);

     System.out.println("input1:");
     Int n=sc.nextint();

     System.out.println("input2:");
     Int k=sc.nextint();

     Int s= (int)math.pow(n,k);
     Int rem=s%10;

     System.out.println("rem");
   }
 }

