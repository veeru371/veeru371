Import java.until.*;
public class Digit_word
 {
  Public static void main (strings args[])
  
   {
    
    Bufferedreader br= new bufferedreader(new input stream reader(system.in));
    int num,count=0,I;
    System.out.println("enter a number to find it is prime or not");
    num= new integer.parselnt(br.readline());
    
    for(i=0;i<num;i++)
     {
       if(num%i==0)
         Count++;
     }
      if(count==2)
         System.out.println("Given number"+num" is prime");
      Else
         System.out.iprintln("Given number"+num" is not prime");
      }
   }
