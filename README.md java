Package mypack;
Public class compare
  {
    Public int getmax(int n,int m)
     {
       if(n>m)
         return n;
       Else
         return m;
     }
    Public int getmin(int n,int m)
     {
       if(n<m)
         return n;
       Else
         return m;
     }
    Public void get equal(int n,int m)
     {
       if(n==m)
         System.out.println("Equal");
       Else
         System.out.println("Not Equal");
      }
  }

